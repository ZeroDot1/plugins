<model>
    <mount>//OPNsense/unboundplus/upstream</mount>
    <description>Unbound Upstream DNS configuration</description>
    <version>0.0.1</version>
    <items>
        <upstreams>
            <upstream type="ArrayField">
                <enable type="BooleanField">
                    <Required>Y</Required>
                    <default>1</default>
                </enable>
                <type type="OptionField">
                    <Required>Y</Required>
                    <default>plain</default>
                    <OptionValues>
                        <plain>Plain<plain>
                        <dot>DoT</dot>
                    </OptionValues>
                </type>
                <server type="NetworkField">
                    <Required>Y</Required>
                </server>
                <port type="PortField">
                    <Required>Y</Required>
                </port>
            </upstream>
        </upstreams>
    </items>
</model>
